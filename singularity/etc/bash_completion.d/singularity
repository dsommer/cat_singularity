    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--add-caps=")
    two_word_flags+=("--add-caps")
    local_nonpersistent_flags+=("--add-caps=")
    flags+=("--allow-setuid")
    local_nonpersistent_flags+=("--allow-setuid")
    flags+=("--app=")
    two_word_flags+=("--app")
    local_nonpersistent_flags+=("--app=")
    flags+=("--apply-cgroups=")
    two_word_flags+=("--apply-cgroups")
    local_nonpersistent_flags+=("--apply-cgroups=")
    flags+=("--bind=")
    two_word_flags+=("--bind")
    two_word_flags+=("-B")
    local_nonpersistent_flags+=("--bind=")
    flags+=("--cleanenv")
    flags+=("-e")
    local_nonpersistent_flags+=("--cleanenv")
    flags+=("--contain")
    flags+=("-c")
    local_nonpersistent_flags+=("--contain")
    flags+=("--containall")
    flags+=("-C")
    local_nonpersistent_flags+=("--containall")
    flags+=("--dns=")
    two_word_flags+=("--dns")
    local_nonpersistent_flags+=("--dns=")
    flags+=("--docker-login")
    local_nonpersistent_flags+=("--docker-login")
    flags+=("--drop-caps=")
    two_word_flags+=("--drop-caps")
    local_nonpersistent_flags+=("--drop-caps=")
    flags+=("--fakeroot")
    flags+=("-f")
    local_nonpersistent_flags+=("--fakeroot")
    flags+=("--home=")
    two_word_flags+=("--home")
    two_word_flags+=("-H")
    local_nonpersistent_flags+=("--home=")
    flags+=("--hostname=")
    two_word_flags+=("--hostname")
    local_nonpersistent_flags+=("--hostname=")
    flags+=("--ipc")
    flags+=("-i")
    local_nonpersistent_flags+=("--ipc")
    flags+=("--keep-privs")
    local_nonpersistent_flags+=("--keep-privs")
    flags+=("--net")
    flags+=("-n")
    local_nonpersistent_flags+=("--net")
    flags+=("--network=")
    two_word_flags+=("--network")
    local_nonpersistent_flags+=("--network=")
    flags+=("--network-args=")
    two_word_flags+=("--network-args")
    local_nonpersistent_flags+=("--network-args=")
    flags+=("--no-home")
    local_nonpersistent_flags+=("--no-home")
    flags+=("--no-init")
    local_nonpersistent_flags+=("--no-init")
    flags+=("--no-nv")
    local_nonpersistent_flags+=("--no-nv")
    flags+=("--no-privs")
    local_nonpersistent_flags+=("--no-privs")
    flags+=("--nohttps")
    local_nonpersistent_flags+=("--nohttps")
    flags+=("--nv")
    local_nonpersistent_flags+=("--nv")
    flags+=("--overlay=")
    two_word_flags+=("--overlay")
    two_word_flags+=("-o")
    local_nonpersistent_flags+=("--overlay=")
    flags+=("--pid")
    flags+=("-p")
    local_nonpersistent_flags+=("--pid")
    flags+=("--pwd=")
    two_word_flags+=("--pwd")
    local_nonpersistent_flags+=("--pwd=")
    flags+=("--scratch=")
    two_word_flags+=("--scratch")
    two_word_flags+=("-S")
    local_nonpersistent_flags+=("--scratch=")
    flags+=("--security=")
    two_word_flags+=("--security")
    local_nonpersistent_flags+=("--security=")
    flags+=("--userns")
    flags+=("-u")
    local_nonpersistent_flags+=("--userns")
    flags+=("--uts")
    local_nonpersistent_flags+=("--uts")
    flags+=("--vm")
    local_nonpersistent_flags+=("--vm")
    flags+=("--vm-cpu=")
    two_word_flags+=("--vm-cpu")
    local_nonpersistent_flags+=("--vm-cpu=")
    flags+=("--vm-err")
    local_nonpersistent_flags+=("--vm-err")
    flags+=("--vm-ram=")
    two_word_flags+=("--vm-ram")
    local_nonpersistent_flags+=("--vm-ram=")
    flags+=("--workdir=")
    two_word_flags+=("--workdir")
    two_word_flags+=("-W")
    local_nonpersistent_flags+=("--workdir=")
    flags+=("--writable")
    flags+=("-w")
    local_nonpersistent_flags+=("--writable")
    flags+=("--writable-tmpfs")
    local_nonpersistent_flags+=("--writable-tmpfs")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

